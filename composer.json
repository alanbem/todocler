{
    "type": "project",
    "name": "alanbem/todocler",
    "description": "event sourced TODO application",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "lexik/jwt-authentication-bundle": "^2.12",
        "nelmio/cors-bundle": "^2.1",
        "php-amqplib/rabbitmq-bundle": "^2.8",
        "phpdocumentor/reflection-docblock": "^5.2",
        "streak/streak-bundle": "dev-master as 1.0",
        "symfony/asset": "^5.3",
        "symfony/console": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/expression-language": "^5.3",
        "symfony/flex": "^1.13",
        "symfony/framework-bundle": "^5.3",
        "symfony/messenger": "^5.3",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "^5.3",
        "symfony/property-info": "^5.3",
        "symfony/proxy-manager-bridge": "^5.3",
        "symfony/runtime": "^5.3",
        "symfony/security-bundle": "^5.3",
        "symfony/serializer": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "symfony/validator": "^5.3",
        "symfony/yaml": "^5.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "friends-of-behat/symfony-extension": "^2.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "qossmic/deptrac-shim": "^0.12.0",
        "rector/rector": "^0.10",
        "roave/security-advisories": "dev-latest",
        "symfony/stopwatch": "^5.3",
        "symfony/web-profiler-bundle": "^5.3"
    },
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Productivity\\": "src/Productivity",
            "Users\\": "src/Users",
            "Shared\\": "src/Shared"
        },
        "files": ["src/Kernel.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Productivity\\": "tests/Productivity",
            "Users\\": "tests/Users"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.3"
        }
    }
}
